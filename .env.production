# ===========================================
# FourthOfficial - Production Environment
# ===========================================

# API Configuration
# Empty to use Vercel rewrites proxy (avoids CORS)
VITE_API_BASE_URL=

# API request timeout in milliseconds
VITE_API_TIMEOUT=30000

# Feature Flags
# Disable DevTools in production for performance
VITE_ENABLE_DEVTOOLS=false

# Never use mock data in production
VITE_ENABLE_MOCK_DATA=false

# Disable debug logging in production
VITE_ENABLE_DEBUG_LOGGING=false

# App Configuration
VITE_APP_NAME=FourthOfficial
VITE_APP_VERSION=1.0.0

# Optional: Analytics & Monitoring
# Add your production Sentry DSN here
# VITE_SENTRY_DSN=

# Add your production GA tracking ID here
# VITE_GA_TRACKING_ID=
